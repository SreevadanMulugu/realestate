    "%%writefile real_estate_chatbot/chat/agent.py\n",
    "import os\n",
    "import re\n",
    "import google.generativeai as genai\n",
    "# Use a relative import for modules within the same package (real_estate_chatbot)\n",
    "from ..db import query as db_query\n",
    "from ..utils import maps_api\n",
    "from . import prompt_templates\n",
    "\n",
    "# API Key is taken from the global USER_API_KEY in the Colab environment (defined in an earlier cell)\n",
    "# This setup is specific to how this Colab notebook is structured.\n",
    "# In a standard Python project, you'd use environment variables or a config file.\n",
    "\n",
    "# Global variable for the Gemini model client\n",
    "gemini_model = None\n",
    "\n",
    "def initialize_gemini_client(api_key: str):\n",
    "    \"\"\"Initializes the Gemini client with the provided API key.\"\"\"\n",
    "    global gemini_model\n",
    "    if gemini_model: # Already initialized\n",
    "        print(\"Gemini client already initialized.\")\n",
    "        return True\n",
    "    try:\n",
    "        current_api_key = getattr(genai, 'API_KEY', None) # More robust way to check configured key\n",
    "        if current_api_key != api_key:\n",
    "             genai.configure(api_key=api_key)\n",
    "             print(f\"Gemini client configured with new API key ending with ...{api_key[-4:]}\")\n",
    "        elif not current_api_key:\n",
    "             genai.configure(api_key=api_key) # Configure if not set at all\n",
    "             print(f\"Gemini client configured with API key ending with ...{api_key[-4:]}\")\n",
    "\n",
    "        gemini_model = genai.GenerativeModel('gemini-1.5-flash-latest')\n",
    "        print(\"Gemini client initializing...\")\n",
    "        gemini_model.generate_content(\"Hello! This is a test call to verify API key and model access.\") \n",
    "        print(\"Gemini client initialized successfully and API key validated.\")\n",
    "        return True\n",
    "    except Exception as e:\n",
    "        print(f\"Error initializing Gemini client or validating API key: {e}\")\n",
    "        gemini_model = None\n",
    "        return False\n",
    "\n",
    "def _call_gemini(prompt: str) -> str | None:\n",
    "    \"\"\"Helper function to call the Gemini API.\"\"\"\n",
    "    if not gemini_model:\n",
    "        print(\"Error: Gemini client not initialized. Please ensure API key is set and valid.\")\n",
    "        return \"Sorry, I'm having trouble connecting to my brain right now. Please try again later.\"\n",
    "    try:\n",
    "        response = gemini_model.generate_content(prompt)\n",
    "        return response.text.strip()\n",
    "    except Exception as e:\n",
    "        print(f\"Error calling Gemini API: {e}\")\n",
    "        # Check for specific API key related errors if possible, e.g. by inspecting 'e'\n",
    "        if \"API_KEY_INVALID\" in str(e) or \"PERMISSION_DENIED\" in str(e) or \"API_KEY_SERVICE_BLOCKED\" in str(e):\n",
    "            return \"There seems to be an issue with the API key configuration. Please check it and restart the application.\"\n",
    "        return f\"Sorry, I encountered an error while processing your request.\"\n",
    "\n",
    "def get_intent(user_query: str, property_names_list: list[str]) -> str:\n",
    "    \"\"\"Classifies the user's query intent using Gemini.\"\"\"\n",
    "    prompt = prompt_templates.INTENT_CLASSIFICATION_PROMPT_TEMPLATE.format(\n",
    "        property_names_list_str=prompt_templates.get_available_properties_for_prompt(property_names_list),\n",
    "        user_query=user_query\n",
    "    )\n",
    "    intent = _call_gemini(prompt)\n",
    "    valid_intents = [\n",
    "        \"DB_QUERY_PRICE\", \"DB_QUERY_LOCATION\", \"DB_QUERY_DETAILS\",\n",
    "        \"NEARBY_QUERY\", \"GENERAL_QUERY\", \"GREETING\", \"THANK_YOU\", \"UNKNOWN\"\n",
    "    ]\n",
    "    if intent not in valid_intents:\n",
    "        print(f\"Warning: LLM returned unexpected intent '{intent}'. Defaulting to UNKNOWN.\")\n",
    "        return \"UNKNOWN\"\n",
    "    return intent\n",
    "\n",
    "def extract_property_entity(user_query: str, property_names_list: list[str]) -> str | None:\n",
    "    \"\"\"Extracts the property name from the user query using Gemini.\"\"\"\n",
    "    prompt = prompt_templates.ENTITY_EXTRACTION_PROMPT_TEMPLATE.format(\n",
    "        property_names_list_str=prompt_templates.get_available_properties_for_prompt(property_names_list),\n",
    "        user_query=user_query\n",
    "    )\n",
    "    entity = _call_gemini(prompt)\n",
    "    if entity == \"Unknown\" or not entity:\n",
    "        for prop_name in property_names_list:\n",
    "            if re.search(r'\\b' + re.escape(prop_name) + r'\\b', user_query, re.IGNORECASE):\n",
    "                return prop_name\n",
    "        return None\n",
    "    # Ensure the extracted entity is one of the known properties for safety\n",
    "    if entity in property_names_list:\n",
    "        return entity\n",
    "    # Fallback if LLM hallucinates a property name not in the list\n",
    "    for prop_name in property_names_list:\n",
    "        if re.search(r'\\b' + re.escape(prop_name) + r'\\b', user_query, re.IGNORECASE):\n",
    "            return prop_name\n",
    "    return None\n",
    "\n",
    "def extract_nearby_entities(user_query: str, property_names_list: list[str]) -> tuple[str | None, str | None]:\n",
    "    \"\"\"Extracts property name and place type for nearby queries using Gemini.\"\"\"\n",
    "    prompt = prompt_templates.ENTITY_EXTRACTION_NEARBY_PROMPT_TEMPLATE.format(\n",
    "        property_names_list_str=prompt_templates.get_available_properties_for_prompt(property_names_list),\n",
    "        user_query=user_query\n",
    "    )\n",
    "    response = _call_gemini(prompt) \n",
    "    property_name_ext, place_type_ext = None, \"amenity\" # Defaults\n",
    "    if response and '|' in response:\n",
    "        parts = response.split('|', 1)\n",
    "        property_name_cand = parts[0].strip()\n",
    "        if property_name_cand != \"Unknown\" and property_name_cand in property_names_list:\n",
    "             property_name_ext = property_name_cand\n",
    "        place_type_ext = parts[1].strip() if parts[1].strip() else \"amenity\"\n",
    "    \n",
    "    # Fallback for property name if LLM fails or extracts poorly\n",
    "    if not property_name_ext:\n",
    "         for prop_name_iter in property_names_list:\n",
    "            if re.search(r'\\b' + re.escape(prop_name_iter) + r'\\b', user_query, re.IGNORECASE):\n",
    "                property_name_ext = prop_name_iter\n",
    "                break\n",
    "    \n",
    "    # Fallback for place_type if LLM fails for it (already defaulted to amenity)\n",
    "    if place_type_ext == \"amenity\" or not place_type_ext:\n",
    "        place_match = re.search(r'\\b(schools?|hospitals?|parks?|restaurants?|shops?|stores?|malls?|amenit(y|ies)|facilities|market|gym)\\b', user_query, re.IGNORECASE)\n",
    "        if place_match:\n",
    "            place_type_ext = place_match.group(1)\n",
    "            # Normalize (e.g., schools -> school)\n",
    "            if place_type_ext.endswith('s') and place_type_ext not in ['gas', 'bus']:\ # simple pluralization\n",
    "                 singular_form = place_type_ext[:-1]\n",
    "                 # check if singular form is more common, e.g. school vs schools for API\n",
    "                 # For this PoC, we'll just use it, but a real system might map to canonical types\n",
    "                 place_type_ext = singular_form \n",
    "\n",
    "    return property_name_ext, place_type_ext\n",
    "\n",
    "\n",
    "def handle_query(user_query: str, api_key_for_init: str) -> str:\n",
    "    \"\"\"Handles the user query by routing it and generating a response.\"\"\"\n",
    "    # Ensure client is initialized (idempotent)\n",
    "    if not initialize_gemini_client(api_key_for_init):\n",
    "         return \"Error: The AI model could not be initialized. Please check the API key and try again.\"\n",
    "\n",
    "    property_names = db_query.get_all_property_names()\n",
    "    intent = get_intent(user_query, property_names)\n",
    "    print(f\"[Agent] User Query: '{user_query}', Intent: {intent}\")\n",
    "\n",
    "    if intent == \"GREETING\":\n",
    "        return prompt_templates.GREETING_RESPONSE_TEMPLATE\n",
    "    if intent == \"THANK_YOU\":\n",
    "        return prompt_templates.THANK_YOU_RESPONSE_TEMPLATE\n",
    "\n",
    "    if intent in [\"DB_QUERY_PRICE\", \"DB_QUERY_LOCATION\", \"DB_QUERY_DETAILS\"]:\n",
    "        property_name = extract_property_entity(user_query, property_names)\n",
    "        if not property_name:\n",
    "            # Try a more general extraction if specific name fails but intent is DB related\n",
    "            return \"I couldn't identify a specific property from our list in your question. Our available properties are: \" + \n",
    "                   f\"{prompt_templates.get_available_properties_for_prompt(property_names)}. Please try again specifying one of these.\"\n",
    "\n",
    "        db_data = db_query.get_property_details_by_name(property_name)\n",
    "        if not db_data:\n",
    "            return f\"Sorry, I don't have information about '{property_name}'.\"\n",
    "\n",
    "        if intent == \"DB_QUERY_PRICE\":\n",
    "            db_data_filtered = {\"price\": db_data.get(\"price\", \"Not available\")}\n",
    "        elif intent == \"DB_QUERY_LOCATION\":\n",
    "            db_data_filtered = {\"location\": db_data.get(\"location\", \"Not available\")}\n",
    "        else: # DB_QUERY_DETAILS\n",
    "            db_data_filtered = db_data\n",
    "\n",
    "        prompt = prompt_templates.FORMAT_DB_RESPONSE_PROMPT_TEMPLATE.format(\n",
    "            user_query=user_query,\n",
    "            property_name=property_name,\n",
    "            db_data=str(db_data_filtered)\n",
    "        )\n",
    "        return _call_gemini(prompt)\n",
    "\n",
    "    elif intent == \"NEARBY_QUERY\":\n",
    "        property_name, place_type = extract_nearby_entities(user_query, property_names)\n",
    "        if not property_name:\n",
    "            return \"Please specify which property you're asking about for nearby places. Our available properties are: \" + \n",
    "                   f\"{prompt_templates.get_available_properties_for_prompt(property_names)}.\"\n",
    "        if not place_type:\n",
    "            place_type = \"amenities\" \n",
    "\n",
    "        location_info = db_query.get_property_location_by_name(property_name)\n",
    "        if not location_info:\n",
    "            return f\"Sorry, I couldn't find location details for '{property_name}' to search for nearby {place_type}.\"\n",
    "\n",
    "        nearby_places_list = maps_api.get_nearby_places(\n",
    "            location_info[\"latitude\"], location_info[\"longitude\"], place_type, property_name\n",
    "        )\n",
    "        prompt = prompt_templates.FORMAT_NEARBY_RESPONSE_PROMPT_TEMPLATE.format(\n",
    "            user_query=user_query,\n",
    "            property_name=property_name,\n",
    "            place_type=place_type,\n",
    "            nearby_places_list=str(nearby_places_list)\n",
    "        )\n",
    "        return _call_gemini(prompt)\n",
    "\n",
    "    elif intent == \"GENERAL_QUERY\":\n",
    "        prompt = prompt_templates.GENERAL_KNOWLEDGE_PROMPT_TEMPLATE.format(user_query=user_query)\n",
    "        return _call_gemini(prompt)\n",
    "\n",
    "    else: # UNKNOWN or other unhandled\n",
    "        # Before returning unknown, make one last attempt with a general prompt if the query is not too short\n",
    "        if len(user_query.split()) > 3: # Arbitrary threshold for a 'meaningful' query\n",
    "            print(f\"[Agent] Intent was {intent}. Trying a general knowledge prompt as a fallback.\")\n",
    "            prompt = prompt_templates.GENERAL_KNOWLEDGE_PROMPT_TEMPLATE.format(user_query=user_query)\n",
    "            fallback_response = _call_gemini(prompt)\n",
    "            if fallback_response and \"sorry, I encountered an error\" not in fallback_response.lower() and \"issue with the API key\" not in fallback_response.lower():\n",
    "                 # Basic check to avoid returning an error message as a valid response\n",
    "                return fallback_response\n",
    "        return prompt_templates.UNKNOWN_QUERY_RESPONSE_TEMPLATE"
   ]
  },
