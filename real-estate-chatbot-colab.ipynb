    "## Step 5: Create the Flask App (`app.py`)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "%%writefile real_estate_chatbot/app.py\n",
    "import os\n",
    "from flask import Flask, request, jsonify, render_template\n",
    "from flask_ngrok import run_with_ngrok # For Colab\n",
    "\n",
    "# Relative imports for modules within the 'real_estate_chatbot' package\n",
    "from .chat.agent import handle_query, initialize_gemini_client\n",
    "\n",
    "# --- Colab Specific: Get API Key from global scope ---\n",
    "# This is how we pass the API key from the Colab cell to the app files.\n",
    "# In a non-Colab environment, you'd use os.getenv(\"GEMINI_API_KEY\") directly.\n",
    "colab_api_key = None\n",
    "try:\n",
    "    # Attempt to get the key set in the earlier Colab cell\n",
    "    # This relies on the global variable USER_API_KEY being accessible here.\n",
    "    # This is a bit of a hack for Colab's environment.\n",
    "    from google.colab import output\n",
    "    colab_api_key = output.eval_፩('USER_API_KEY') # Access variable from notebook's global scope\n",
    "except: # Fallback or if not in Colab, though this script is designed for it\n",
    "    pass\n",
    "if not colab_api_key:\n",
    "    print(\"Warning: API Key not found from Colab global scope. Trying os.getenv.\")\n",
    "    colab_api_key = os.getenv(\"GEMINI_API_KEY\") # Fallback for local testing of this script\n",
    "# --- End Colab Specific ---\n",
    "\n",
    "\n",
    "app = Flask(__name__, template_folder='web', static_folder='web/static')\n",
    "run_with_ngrok(app)  # Start ngrok when app is run, specific for Colab\n",
    "\n",
    "API_KEY_INITIALIZED = False\n",
    "\n",
    "# It's better to initialize once when the app context is available or on first request.\n",
    "# Using before_first_request is cleaner than global initialization for Flask apps.\n",
    "@app.before_first_request\n",
    "def initial_setup():\n",
    "    global API_KEY_INITIALIZED\n",
    "    if colab_api_key:\n",
    "        print(f\"Attempting to initialize Gemini client with API key ending ...{colab_api_key[-4:]}\")\n",
    "        if initialize_gemini_client(colab_api_key):\n",
    "            API_KEY_INITIALIZED = True\n",
    "            print(\"Gemini client initialization successful via before_first_request.\")\n",
    "        else:\n",
    "            print(\"Failed to initialize Gemini client via before_first_request. Chatbot may not function.\")\n",
    "            API_KEY_INITIALIZED = False # Explicitly set to False\n",
    "    else:\n",
    "        print(\"GEMINI_API_KEY not found (checked Colab global and environment). Chatbot will not function.\")\n",
    "        API_KEY_INITIALIZED = False\n",
    "\n",
    "@app.route('/')\n",
    "def home():\n",
    "    return render_template('chatbot.html')\n",
    "\n",
    "@app.route('/chat', methods=['POST'])\n",
    "def chat_endpoint():\n",
    "    global API_KEY_INITIALIZED\n",
    "    if not API_KEY_INITIALIZED:\n",
    "        # Try to re-initialize if it failed before or key might have been set late\n",
    "        print(\"API key not initialized at /chat endpoint. Attempting re-initialization...\")\n",
    "        if colab_api_key and initialize_gemini_client(colab_api_key):\n",
    "            API_KEY_INITIALIZED = True\n",
    "            print(\"Gemini client re-initialized successfully at /chat endpoint.\")\n",
    "        else:\n",
    "            error_msg = \"AI model not initialized. \"\n",
    "            if not colab_api_key:\n",
    "                error_msg += \"API Key is missing. \"\n",
    "            else:\n",
    "                error_msg += \"API Key might be invalid or there was an issue during initialization. \"\n",
    "            error_msg += \"Please check the Colab setup and API key.\"\n",
    "            print(f\"Error at /chat: {error_msg}\")\n",
    "            return jsonify({\"error\": error_msg}), 500\n",
    "            \n",
    "    if not request.is_json:\n",
    "        return jsonify({\"error\": \"Request must be JSON\"}), 400\n",
    "\n",
    "    data = request.get_json()\n",
    "    user_query = data.get('query')\n",
    "\n",
    "    if not user_query:\n",
    "        return jsonify({\"error\": \"Missing 'query' in request data\"}), 400\n",
    "\n",
    "    try:\n",
    "        # Pass the API key to handle_query, which then passes to initialize_gemini_client if needed\n",
    "        response_text = handle_query(user_query, colab_api_key)\n",
    "        return jsonify({\"response\": response_text})\n",
    "    except Exception as e:\n",
    "        print(f\"Error during handle_query in /chat: {e}\")\n",
    "        return jsonify({\"error\": f\"An internal error occurred: {str(e)}\"}), 500\n",
    "\n",
    "@app.route('/health')\n",
    "def health_check():\n",
    "    # This provides the status of the API key initialization for debugging from the ngrok URL\n",
    "    return jsonify({\"status\": \"ok\", \"api_key_initialized_status\": API_KEY_INITIALIZED}), 200\n",
    "\n",
    "# The if __name__ == '__main__': block is not strictly necessary for Colab execution via app.run()\n",
    "# but good practice for structure if this file were run directly elsewhere.\n",
    "def run_app():\n",
    "    # The actual app run is usually done in a separate cell in Colab for clarity\n",
    "    print(\"Flask app starting with ngrok...\")\n",
    "    print(f\"API Key for Flask app (ends with): ...{colab_api_key[-4:] if colab_api_key else 'NOT_SET'}\")\n",
    "    app.run() # This will be executed by the last cell in Colab"
   ]
